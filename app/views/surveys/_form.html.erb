<%= simple_form_for(@survey) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    
    <select name="Project">
    	<%= @current_projects.each do |project| %>
    		<option value="<%=project.title%>"><%=project.title%></option>
    	<%end%>
    </select>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
